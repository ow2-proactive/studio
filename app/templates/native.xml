<?xml version="1.0" encoding="UTF-8"?>
<job
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="urn:proactive:jobdescriptor:3.3"
        xsi:schemaLocation="urn:proactive:jobdescriptor:3.3 http://www.activeeon.com/public_content/schemas/proactive/jobdescriptor/3.3/schedulerjob.xsd"
        name="Native_job"
        projectName=""
        priority="normal"
        cancelJobOnError="false">

    <taskFlow>
        <task name="Native">
            <nativeExecutable>
                <staticCommand value="/bin/bash" >
                    <arguments>
                        <argument value="-c"/>
                        <argument value="sleep 1"/>
                    </arguments>
                </staticCommand>
            </nativeExecutable>
        </task>
    </taskFlow>
</job>