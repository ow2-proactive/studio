<?xml version="1.0" encoding="UTF-8"?>
<job xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="urn:proactive:jobdescriptor:3.9"
	xsi:schemaLocation="urn:proactive:jobdescriptor:3.9 http://www.activeeon.com/public_content/schemas/proactive/jobdescriptor/3.9/schedulerjob.xsd"
	name="script_javascript" projectName="" priority="normal" onTaskError="continueJobExecution">
	<description>
		A workflow that executes javaScript in JVM.
	</description>
	<taskFlow>
		<task name="Javascript_Task">
			<description>
				The simplest task, ran by a javascript engine.
			</description>
			<scriptExecutable>
				<script>
					<code language="javascript">
                        <![CDATA[
print("Hello World!")
]]>
					</code>
				</script>
			</scriptExecutable>
		</task>
	</taskFlow>
</job>